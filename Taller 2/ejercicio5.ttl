        a rdf:Seq;
        rdf:_1 "Alan Turing";
        rdf:_2 "John Von Neumann";
        rdf:_3 "Steve Jobs";
    ];
    event:fecha [
        event:mes [
            a rdf:Alt;
            rdf:_1 "Enero";
            rdf:_2 "Febrero";
            rdf:_3 "Marzo";
            rdf:_4 "Abril";
            rdf:_5 "Mayo";
            rdf:_6 "Junio";
            rdf:_7 "Julio";
            rdf:_8 "Agosto";
            rdf:_9 "Septiembre";
            rdf:_10 "Octubre";
            rdf:_11 "Noviembre";
            rdf:_12 "Diciembre";
        ];
        event:dia [
            a rdf:Alt;
            rdf:_1 "Domingo";
            rdf:_2 "Lunes";
            rdf:_3 "Martes";
            rdf:_4 "Miercoles";
            rdf:_5 "Jueves";
            rdf:_6 "Viernes";
            rdf:_7 "Sabado";
        ]
    ];
    event:empleados [
        event:meseros ("Carlos" "Ciro");
        event:ayudantes ("Santiago" "Alejandro");
    ].
